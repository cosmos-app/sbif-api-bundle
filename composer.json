{
    "name": "cosmos-app/sbif-api-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Sbif Api Bundle",
    "keywords": [
        "sbif",
        "api",
        "bundle"
    ],
    "homepage": "https://github.com/cosmos-app/sbif-api-bundle",
    "authors": [
        {
            "name": "HÃ©ctor Rojas",
            "email": "hector.d.rojas.s@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CosmosApp\\SbifApiBundle\\": ""
        }
    },
    "autoload-dev": {
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "~2.7.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2",
        "phpunit/phpunit": "^4"
    },
    "config": {
        "php": "5.5.9",
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
